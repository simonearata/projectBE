{
  "kind": "collectionType",
  "collectionName": "stocks",
  "info": {
    "singularName": "stock",
    "pluralName": "stocks",
    "displayName": "Stock"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "isin": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "quantity": {
      "type": "decimal"
    },
    "wallets": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::wallet.wallet",
      "inversedBy": "stocks"
    }
  }
}
